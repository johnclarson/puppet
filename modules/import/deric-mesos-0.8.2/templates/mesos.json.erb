{
    "default": {
        "response_timeout": <%= @response_timeout %>,
        "log_file": <%= @log_file %>,
        "log_level": "<%= @log_level %>",
        "max_workers": <%= @max_workers %>,
<% if !@zookeeper.nil? && !@zookeeper.empty? -%>
        "master": "<%= @zookeeper %>",
<% else -%>
        "master": "<%= @master %>:<%= @master_port %>",
<% end -%>
        "debug": "<%= @debug %>",
        "scheme": "<%= @scheme %>"
    },
    "profile": "default"
}